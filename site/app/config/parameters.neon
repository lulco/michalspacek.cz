parameters:
	notifier:
		vrana:
			noSpam: nospam
			successMarker: 'Děkuji za přihlášku na školení'
	domain:
		default: www
		sharedStaticRoot: https://www.%domain.root%
	reporting:
		enabledLocally: false
		contentSecurityPolicy: https://report-uri.io/report/me
		xXssProtection: https://report-uri.io/report/me
	contentSecurityPolicy:
		www:
			default-src: "'self'"
			img-src: "'self' https://www.google-analytics.com"
			script-src: "'self' https://www.google-analytics.com https://speakerdeck.com"
			frame-src: 'https://www.slideshare.net https://speakerdeck.com https://www.youtube-nocookie.com https://player.vimeo.com https://slideslive.com'
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		admin:
			default-src: https://www.%domain.root%
			img-src: 'https://www.%domain.root% data:'
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		heartbleed:
			default-src: https://www.%domain.root%
			img-src: 'https://www.%domain.root% https://www.google-analytics.com'
			script-src: 'https://www.%domain.root% https://www.google-analytics.com'
			style-src: "https://www.%domain.root% 'unsafe-inline'"
			form-action: "'none'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		cotel:
			default-src: 'https://www.%domain.root%'
			style-src: "https://www.%domain.root% 'unsafe-inline'"
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
	extraHeaders:
		www:
				X-XSS-Protection: '1; mode=block; report=%reporting.xXssProtection%'
		admin:
				X-XSS-Protection: '1; mode=block; report=%reporting.xXssProtection%'
		heartbleed:
				X-XSS-Protection: '1; mode=block; report=%reporting.xXssProtection%'
		cotel:
				X-XSS-Protection: '1; mode=block; report=https://www.%domain.root%/report/xss'
	vatRate: 0.21
	ares:
		url: 'https://wwwinfo.mfcr.cz/cgi-bin/ares/darv_bas.cgi?ico=%s'
	registerUz:
		rootUrl: 'http://www.registeruz.sk/cruz-public/api/'
