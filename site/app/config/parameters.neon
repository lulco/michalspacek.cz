parameters:
	notifier:
		vrana:
			noSpam: nospam
			successMarker: 'Děkuji za přihlášku na školení'
	domain:
		default: www
		sharedStaticRoot: https://www.%domain.root%
	contentSecurityPolicy:
		www:
			default-src: "'self'"
			img-src: "'self' https://www.google-analytics.com"
			script-src: "'self' https://www.google-analytics.com https://speakerdeck.com"
			style-src: "'self' 'unsafe-inline'"
			child-src: 'https://www.slideshare.net https://speakerdeck.com https://www.youtube-nocookie.com https://player.vimeo.com'
			form-action: "'self'"
			report-uri: https://report-uri.io/report/me
		admin:
			default-src: https://www.%domain.root%
			img-src: 'https://www.%domain.root% data:'
			style-src: "https://www.%domain.root% 'unsafe-inline'"
			form-action: "'self'"
			report-uri: https://report-uri.io/report/me
		heartbleed:
			default-src: https://www.%domain.root%
			img-src: 'https://www.%domain.root% https://www.google-analytics.com'
			script-src: 'https://www.%domain.root% https://www.google-analytics.com'
			style-src: "https://www.%domain.root% 'unsafe-inline'"
			form-action: "'none'"
			report-uri: https://report-uri.io/report/me
		cotel:
			default-src: 'https://www.%domain.root%'
			style-src: "https://www.%domain.root% 'unsafe-inline'"
			form-action: "'self'"
			report-uri: https://report-uri.io/report/me
	extraHeaders:
		www:
				X-XSS-Protection: '1; mode=block; report=/report/xss'
		admin:
				X-XSS-Protection: '1; mode=block; report=https://www.%domain.root%/report/xss'
		heartbleed:
				X-XSS-Protection: '1; mode=block; report=https://www.%domain.root%/report/xss'
		cotel:
				X-XSS-Protection: '1; mode=block; report=https://www.%domain.root%/report/xss'
	vatRate: 0.21
	ares:
		url: 'https://wwwinfo.mfcr.cz/cgi-bin/ares/darv_bas.cgi?ico=%s'
	registerUz:
		rootUrl: 'http://www.registeruz.sk/cruz-public/api/'
