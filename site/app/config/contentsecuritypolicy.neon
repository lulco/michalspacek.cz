parameters:
	contentSecurityPolicy:
		default_default:
			default-src: "'none'"
			img-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
			script-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
			style-src:
				- %domain.contentSecurityPolicySelf%
			frame-ancestors: "'none'"
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		talks_talk:
			- %contentSecurityPolicy.default_default%
			script-src:
				- %contentSecurityPolicy.default_default.script-src%
				- https://speakerdeck.com
			child-src:
				- https://www.slideshare.net
				- https://speakerdeck.com
				- https://www.youtube-nocookie.com
				- https://player.vimeo.com
				- https://slideslive.com
			frame-src: %contentSecurityPolicy.talks_talk.child-src%
		interviews_interview:
			- %contentSecurityPolicy.default_default%
			child-src:
				- https://www.youtube-nocookie.com
				- https://player.vimeo.com
			frame-src: %contentSecurityPolicy.interviews_interview.child-src%
		trainings_training:
			- %contentSecurityPolicy.default_default%
			connect-src: 'https://api.%locales.rootDomainMapping.cz%'
		admin_default_default:
			default-src: "'none'"
			img-src:
				- %domain.contentSecurityPolicySelf%
			script-src:
				- %domain.contentSecurityPolicySelf%
			style-src:
				- %domain.contentSecurityPolicySelf%
			frame-ancestors: "'none'"
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		admin_info_php:
			- %contentSecurityPolicy.admin_default_default%
			img-src:
				- %domain.contentSecurityPolicySelf%
				- 'data:'
		api_default_default:
			default-src: "'none'"
			frame-ancestors: "'none'"
			form-action: "'none'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		webleed_default_default:
			default-src: "'none'"
			img-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
				- 'blob:'
			script-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
			style-src:
				- %domain.contentSecurityPolicySelf%
				- "'unsafe-inline'"
			frame-ancestors: "'none'"
			form-action: "https://export.highcharts.com"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
		upckeys_default_default:
			default-src: "'none'"
			img-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
			script-src:
				- %domain.contentSecurityPolicySelf%
				- https://www.google-analytics.com
			style-src:
				- %domain.contentSecurityPolicySelf%
			frame-ancestors: "'none'"
			form-action: "'self'"
			upgrade-insecure-requests:
			block-all-mixed-content:
			report-uri: %reporting.contentSecurityPolicy%
